import java.util.Scanner;

public class Main {
    public static void main(String[] args){
 
        Scanner sc = new Scanner(System.in);
        
        int time = sc.nextInt();
        int speed = sc.nextInt();
        double distance = time*speed;
        double liters = distance / 12;

        
        System.out.printf("%.3f%n", liters);
      }   
    }
