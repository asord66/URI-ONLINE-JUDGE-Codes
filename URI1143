import java.util.Scanner;
import java.lang.Math;

public class Main{
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        
        int limit = sc.nextInt();
        
        for (int i = 1; i <= limit; i++) {
            System.out.println( i + " " + i*i + " " + i*i*i);
        }
      }
    }
