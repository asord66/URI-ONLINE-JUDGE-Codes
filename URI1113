import java.util.Scanner;
import java.lang.Math;
public class Main {
 
    public static void main(String[] args){
        
        Scanner sc = new Scanner(System.in);
        
        
        while(sc.hasNext()){
            
        int num1 = sc.nextInt();
        int num2 = sc.nextInt();
        
            if (num1==num2) {
                return;
            }else{
              if (num1>num2) {
            System.out.println("Decrescente");
        }else{
            System.out.println("Crescente");
        } 
        }   
            }
     
}
}
