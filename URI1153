import java.util.Scanner;

public class Main{
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        
        int num = sc.nextInt(),result=num;
        
        for (int i = num; i > 1; i--){
            result *= i-1;
        }
        System.out.println(result);   
    }
}
